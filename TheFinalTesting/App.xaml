<Application x:Class="TheFinalTesting.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:TheFinalTesting"
             xmlns:vm="clr-namespace:TheFinalTesting.ViewModel"
             StartupUri="View/FrmMain.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Dictionary/Share.xaml"/>
                <ResourceDictionary Source="/Dictionary/Style.xaml"/>
                <ResourceDictionary Source="/Dictionary/Template.xaml"/>
                
            </ResourceDictionary.MergedDictionaries>
            <vm:LocatorViewModel x:Key="locator"/>
        </ResourceDictionary>
    </Application.Resources>
</Application>
