<Application x:Class="PssHighLowTemperature.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:vm="clr-namespace:PssHighLowTemperature.ViewModel"
             xmlns:a="clr-namespace:PssHighLowTemperature.Assistant"
             xmlns:local="clr-namespace:PssHighLowTemperature"
             StartupUri="View\FrmMain.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Dictionary/Share.xaml"/>
                <ResourceDictionary Source="/Dictionary/Template.xaml"/>
                <ResourceDictionary Source="/Dictionary/Style.xaml"/>
                
            </ResourceDictionary.MergedDictionaries>
            <vm:LocatorViewModel x:Key="locator"/>
            <a:ImageConverter x:Key="imageCon"/>
        </ResourceDictionary>
        
    </Application.Resources>
    
</Application>
